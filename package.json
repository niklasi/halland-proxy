{
  "name": "halland-proxy",
  "productName": "Halland Proxy",
  "version": "0.0.8",
  "description": "x-platform web debugging proxy and testing tool.",
  "scripts": {
    "test": "standard && babel-tap test/**/test-*.js",
    "dev": "babel src --out-dir app --copy-files --watch --source-maps",
    "start": "npm run build && electron .",
    "build": "babel src --out-dir app --copy-files --source-maps",
    "pack": "NODE_ENV=production npm run build && build --dir",
    "dist": "NODE_ENV=production npm run build && build --publish onTagOrDraft"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niklasi/halland-proxy.git"
  },
  "keywords": [
    "proxy",
    "debugging",
    "testing",
    "http(s)"
  ],
  "author": {
    "name": "Niklas Ingholt",
    "email": "niklas@ingholt.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/niklasi/hallandproxy/issues"
  },
  "homepage": "https://github.com/niklasi/hallandproxy#readme",
  "main": "app/index.js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-react": "^6.16.0",
    "babel-tap": "^5.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.4.8",
    "electron-builder": "^8.0.0",
    "enzyme": "^2.5.1",
    "react-addons-test-utils": "^15.3.2",
    "request": "^2.75.0",
    "standard": "^8.0.0",
    "tap": "^7.1.2"
  },
  "dependencies": {
    "bytewise": "^1.1.0",
    "connect": "^3.5.0",
    "hexer": "^1.5.0",
    "ip": "^1.1.3",
    "leveldown": "^1.4.6",
    "levelup": "^1.3.2",
    "material-ui": "^0.15.4",
    "memdown": "^1.2.2",
    "mkdirp": "^0.5.1",
    "react": "^15.3.1",
    "react-addons-perf": "^15.3.2",
    "react-addons-pure-render-mixin": "^15.3.2",
    "react-addons-shallow-compare": "^15.3.2",
    "react-dom": "^15.3.1",
    "react-highlight": "^0.9.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^2.0.0",
    "react-ui": "^0.6.2",
    "react-virtualized": "^8.5.1",
    "redux": "^3.2.1",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "through2": "^2.0.1"
  },
  "build": {
    "files": [
      "app/",
      "src/",
      "node_modules",
      "!**/node_modules/*/{README.md,README,readme.md,readme,test}",
      "!**/node_modules/.bin",
      "!**/*.{o,hprof,orig,pyc,pyo,rbc}",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,__pycache__,thumbs.db}"
    ],
    "asar": false,
    "appId": "com.ingholt.hallandproxy",
    "category": "public.app-category.development",
    "dmg": {
      "publish": [
        "github"
      ],
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "remoteReleases": "https://github.com/nilkasi/hallandproxy"
    },
    "linux": {
      "publish": [
        "github"
      ],
      "publish": null,
      "target": [
        "deb",
        "AppImage"
      ]
    }
  }
}
